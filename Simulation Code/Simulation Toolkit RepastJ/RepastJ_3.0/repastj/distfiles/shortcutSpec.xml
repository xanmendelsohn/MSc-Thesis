<shortcuts>
	<programGroup defaultName="Repast 3"
		location="applications"/>
	<shortcut name="Uninstall Repast"
		subgroup="/RepastJ"
		target="javaw.exe"
		commandLine=' -jar "$INSTALL_PATH/Uninstaller/uninstaller.jar"'
		workingDirectory="$INSTALL_PATH/repastj"
		initialState="noShow"
		programGroup="yes"/>
	<shortcut
		name="RepastJ"
		target="javaw.exe"
		subgroup="/RepastJ"
		commandLine=' -jar "$INSTALL_PATH/repastj/repastj.jar"'
		workingDirectory="$INSTALL_PATH/repastj/"
		iconFile="$INSTALL_PATH/repast.ico"
		initialState="noShow"
		programGroup="yes">	
		<createForPack name="RepastJ"/>
	</shortcut>
	<shortcut
		name="How-to"
		subgroup="Docs"
		target="$INSTALL_PATH/repastj/docs/how_to/how_to.html"
		workingDirectory="$INSTALL_PATH/repastj/"
		initialState="normal"
		programGroup="yes">
		<createForPack name="Docs"/>
	</shortcut>
	<shortcut
		name="API"
		target="$INSTALL_PATH/repastj/docs/api/index.html"
		initialState="normal"
		programGroup="yes">
		<createForPack name="Docs"/>
	</shortcut>
